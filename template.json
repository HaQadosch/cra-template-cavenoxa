{
  "package": {
    "husky": {
      "hooks": {
        "pre-commit": "pretty-quick --staged && npm run test"
      }
    },
    "prettier": {
      "printWidth": 120,
      "singleQuote": true,
      "jsxSingleQuote": true,
      "trailingComma": "all"
    },
    "dependencies": {
      "@reduxjs/toolkit": "^1.2.5",
      "@testing-library/jest-dom": "^5.1.1",
      "@testing-library/react": "^9.4.0",
      "@testing-library/user-event": "^10.0.0",
      "@types/jest": "^25.1.3",
      "@types/node": "^13.7.4",
      "@types/react": "^16.9.21",
      "@types/react-dom": "^16.9.5",
      "@types/react-redux": "^7.1.7",
      "immer": "^5.3.6",
      "react": "^16.12.0",
      "react-dom": "^16.12.0",
      "react-redux": "^7.2.0",
      "react-scripts": "3.4.0",
      "redux-saga": "^1.1.3",
      "typescript": "^3.8.2"
    },
    "scripts": {
      "start": "react-scripts start",
      "build": "react-scripts build",
      "postbuild": "mv build ../../.dist",
      "build:local": "rm -rf ../../.dist && npm run build",
      "sync": "npm run build:local && aws s3 sync ../../.dist/ s3://cx-shared-frontend-bucket-dev/$(echo $USER) --profile saml",
      "postsync": "echo please visit https://cx-shared-frontend-bucket-dev.s3.eu-central-1.amazonaws.com/$(echo $USER)/index.html",
      "test": "react-scripts test --watchAll=false",
      "test:watch": "react-scripts test",
      "eject": "react-scripts eject"
    }
  }
}